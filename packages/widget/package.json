{
  "name": "aquameta-widget",
  "version": "0.1.11",
  "description": "Widget rendering framework built on top of Aquameta",
  "main": "dist/build.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "rollup -c --experimentalDynamicImport",
    "build-datum": "webpack && mv dist/main.js data/widget.dep/datum/js",
    "dev": "npm run build -- -w",
    "start": "aquameta",
    "old-demo": "node demo/server.js",
    "sync": "import -d data",
    "sync-w": "nodemon -w data -e '' -x 'yarn sync'",
    "test": "echo \"tests!\""
  },
  "keywords": [
    "widget",
    "datum",
    "aquameta",
    "postgres"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micburks/aquameta-widget.git"
  },
  "homepage": "https://github.com/micburks/aquameta-widget",
  "author": "Mickey Burks",
  "license": "MIT",
  "dependencies": {
    "aquameta-datum": "^0.1.11",
    "dot": "^1.1.2",
    "jsx-transform": "^2.4.0",
    "parse5": "^4.0.0",
    "ramda": "^0.25.0",
    "scope-css": "^1.1.0"
  },
  "devDependencies": {
    "aquameta": "^0.1.11",
    "aquameta-sync": "^0.1.11",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "express": "^4.16.3",
    "mocha": "^5.0.0",
    "nodemon": "^1.19.0",
    "nyc": "^11.4.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "veggie": "^2.4.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "gitHead": "029cfb95244fc439b1fa1f28ab3c200852cdf53f"
}
