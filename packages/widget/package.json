    "scope-css": "^1.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "express": "^4.16.3",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-builtins": "^2.1.2",
