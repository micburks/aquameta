    "just-curry-it": "^3.1.0",
    "pg": "^7.4.1",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "create-universal-package": "^4.1.0",
    "nodemon": "^1.18.11",
    "veggie": "^2.4.1"
