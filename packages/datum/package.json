{
  "name": "aquameta-datum",
  "version": "0.1.19",
  "description": "Service layer for the Aquameta database API",
  "sideEffects": false,
  "type": "module",
  "main": "dist-node/index.js",
  "exports": {
    "./node": "dist-node/index.js",
    "./browser": "dist-browser/index.js"
  },
  "scripts": {
    "build-browser": "rm -rf dist-browser && babel --config-file ./babel.browser.js src -d dist-browser",
    "build-node": "rm -rf dist-node && babel --config-file ./babel.node.js src -d dist-node",
    "build": "rm -rf dist-* && yarn build-browser && yarn build-node",
    "build-w": "yarn build-node -w",
    "start": "./demo/server.js",
    "preversion": "yarn test",
    "version": "yarn build",
    "postversion": "git push && git push --tags",
    "test": "these-are-tests dist-node/__tests__",
    "test-w": "nodemon -w dist-node -x 'yarn test'",
    "watch": "run-p build-w test-w"
  },
  "keywords": [
    "datum",
    "aquameta",
    "postgres"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micburks/aquameta-datum.git"
  },
  "homepage": "https://github.com/micburks/aquameta-datum",
  "author": "Mickey Burks",
  "license": "MIT",
  "dependencies": {
    "@micburks/pg": "^8.0.1",
    "just-compose": "^1.1.0",
    "just-curry-it": "^3.1.0",
    "node-fetch": "^2.3.0",
    "ramda": "^0.26.1",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-flow": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.8.3",
    "babel-plugin-transform-cup-globals": "^1.0.2",
    "babel-plugin-transform-prune-unused-imports": "^1.0.1",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "flow-bin": "^0.104.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "postgres": "^1.0.2",
    "sinon": "^7.3.2",
    "these-are-tests": "^1.0.4",
    "veggie": "^2.4.1"
  },
  "files": [
    "dist-browser",
    "dist-node"
  ]
}
