{
  "name": "aquameta-datum",
  "version": "0.0.6",
  "description": "Service layer for the Aquameta database API",
  "main": "dist/datum.js",
  "module": "dist/datum.es.js",
  "jsnext:main": "dist/datum.es.js",
  "browser": "dist/datum.browser.js",
  "scripts": {
    "build": "rollup -c rollup/rollup.config.js",
    "build:es": "rollup -c rollup/rollup.config.es.js",
    "build:browser": "rollup -c rollup/rollup.config.browser.js",
    "build:all": "npm run build && npm run build:es && npm run build:browser",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "demo": "nodemon --watch dist demo/server.js",
    "preversion": "npm run test",
    "version": "npm run build:all",
    "postversion": "git push && git push --tags",
    "pretest": "npm run build",
    "test": "nyc mocha",
    "watch:browser": "npm run build:browser -- -w"
  },
  "keywords": [
    "datum",
    "aquameta",
    "postgres"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micburks/aquameta-datum.git"
  },
  "homepage": "https://github.com/micburks/aquameta-datum",
  "author": "Mickey Burks",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.3",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "uglify-es": "github:mishoo/UglifyJS2#harmony"
  },
  "dependencies": {
    "aquameta-query": "0.0.6"
  },
  "files": [
    "dist"
  ]
}
