{
  "name": "aquameta-datum",
  "version": "0.1.19",
  "description": "Service layer for the Aquameta database API",
  "sideEffects": false,
  "type": "module",
  "main": "./dist-browser/index.js",
  "exports": {
    ".": "./dist-node/index.js",
    "./node": "./dist-node/index.js",
    "./browser": "./dist-browser/index.js"
  },
  "scripts": {
    "build": "./build.sh",
    "build-w": "nodemod -w src -x 'yarn build'",
    "start": "./demo/server.js",
    "preversion": "yarn test",
    "version": "yarn build",
    "postversion": "git push && git push --tags",
    "postinstall": "cat setup.sql | psql aquameta",
    "test": "these-are-tests dist-node/__tests__",
    "test-w": "nodemon -w dist-node -x 'yarn test'",
    "watch": "run-p build-w test-w"
  },
  "keywords": [
    "datum",
    "aquameta",
    "postgres"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micburks/aquameta.git"
  },
  "homepage": "https://github.com/micburks/aquameta/tree/master/packages/datum",
  "author": "Mickey Burks",
  "license": "MIT",
  "dependencies": {
    "@micburks/pg": "^8.0.1",
    "just-compose": "^1.1.0",
    "just-curry-it": "^3.1.0",
    "node-fetch": "^2.6.0",
    "parse5": "^6.0.0",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-flow": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.8.3",
    "babel-plugin-transform-cup-globals": "^1.0.2",
    "babel-plugin-transform-prune-unused-imports": "^1.0.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "flow-bin": "^0.120.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postgres": "^1.0.2",
    "sinon": "^9.0.1",
    "these-are-tests": "^1.0.4",
    "veggie": "^2.4.1"
  },
  "files": [
    "dist-browser",
    "dist-node",
    "src"
  ]
}
